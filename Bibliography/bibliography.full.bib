  publisher = {Academic Press},
  year = {1994},
  editor = {Scott, Marilyn E. and Smith, Gary},
  pages = {337-346},
  address = {San Diego},
  annote = {Myxoma virus is a poxvirus that produces bengn fibromases in certain
	members of an american genus of rabbits Sylvilagus and causes severe
	disease in eurpoean rabbit Oryctolagus cuniculus. Most famous strain
	came from brazil and were introduced in to Australia and France in
	1950 and 52, respectively. Transmission occurs via mosquitos which
	feed on lesions of infected hosts rabbit flea in europe is also an
	important vector and direct trans. can occur under crowded/close
	conditions. Australian intro at first seemed to locally die out,
	but then spread like wildfire spreading out over 1000 miles north-south
	and also east-west in just three months. Besides field obs. an experimental
	inoculation was set up and is sketched out. Original virus had 99.8PERCENT
	host mort, this soon dropped to 90PERCENT within just a few months
	before/during a second outbreak. Laboratory research on virulence
	and trans. via mosquitos showed that trans efficiency was related
	to concentration of virus in skin near lesions. "Researchers found
	that the virulent and moderately virulent strains were at about the
	same concentration in the skin, HOwever trans. occurred for only
	about 4 d. before host death in virul. strain colmpared with 16-25
	d for the less virul. strain." Virus Evolution: Trans. of strongly
	attenuated (i.e. very low vir) strains was impossible or difficult
	due to the lack of lesion formation or b/c they healed quickly. Ability
	of virus to make it through overwintering period when trans. is unlikely
	is also apperently important. (seems like little theoretical studies
	of this nature have been done) Six virus virulence grades are based
	on survival time of lab innoculated hosts. Grade I was most virulent,
	field pops almost immediately evolved to grade III and remained there
	for more than 30yrs. Host Evolution: Rabbits evolved resistance to
	Grade I and Grade III in the wild. Similar results found in lab.
	breeding experiments (i.e. evol. of resist. to grade I.) Additional
	evidence that the virus has eventually evolved greater virulence
	levels (i.e. more grade I and II) in regions where there is greater
	host resistance. }
}

@ARTICLE{FentonAndRands2004,
  author = {Fenton, A. and Rands, S. A.},
  title = {Optimal parasite infection strategies: a state-dependent approach},
  journal = {International Journal For Parasitology},
  year = {2004},
  volume = {34},
  pages = {813-821},
  abstract = {Many macroparasites spend a crucial phase of their life-cycle as free-living
	infective stages. Despite their importance, however, little theoretical
	work has considered how evolution may shape the behaviour of these
	stages. Here, we develop what we believe to be the first stochastic
	dynamic programming model of parasite life-history strategies to
	investigate how a trade-off between resource depletion and host encounter
	rate may shape the optimal infection strategy of a macroparasite.
	The optimal strategy depends strongly on the probability of host
	contact and, depending on the relative costs and benefits, macroparasites
	should adopt either a passive 'ambushing' (sit-and-wait) strategy,
	an active cruising strategy or a mixed strategy with an initial cruising
	phase, followed by a switch to ambushing when energy reserves fall
	to a threshold level. Under no circumstances does the model predict
	ambush-then-cruise. We use our model to help interpret previously
	published data on entompathogenic nematode foraging behaviour, showing
	how this framework could facilitate our understanding of macroparasite
	behaviour during this key stage of the life-cycle. (C) 2004 Australian
	Society for Parasitology Inc. Published by Elsevier Ltd. All rights
	reserved. }
}

@ARTICLE{FereaEtAl1999,
  author = {Ferea, T. L. and Botstein, D. and Brown, P. O. and Rosenzweig, R.
	F.},
  title = {Systematic changes in gene expression patterns following adaptive
	evolution in yeast},
  journal = {Proceedings of the National Academy of Sciences of the United States
	of America},
  year = {1999},
  volume = {96},
  pages = {9721-9726},
  abstract = {Culturing a population of Saccharomyces cerevisiae for many generations
	under conditions to which it is not optimally adapted selects for
	fitter genetic variants, This simple experimental design provides
	a tractable model of adaptive evolution under natural selection.
	Beginning,vith a clonal, founding population, independently evolved
	strains were obtained from three independent cultures after continuous
	aerobic growth in glucose-limited chemostats for more than 250 generations.
	DNA microarrays were used to compare genome-wide patterns of gene
	expression in the evolved strains and the parental strain. Several
	hundred genes were found to have significantly altered expression
	in the evolved strains, Many of these genes showed similar alterations
	in their expression in all three evolved strains, Genes with altered
	expression in the three evolved strains included genes involved in
	glycolysis, the tricarboxylic acid cycle, oxidative phosphorylation,
	and metabolite transport. These results are consistent with physiological
	observations and indicate that increased fitness is acquired by altering
	regulation of central metabolism such that less glucose is fermented
	and more glucose is completely oxidized.}
}

