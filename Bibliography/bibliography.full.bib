@article{RhindEtAl2011,
	author	    = {Rhind, N. and Chen, Z. H. and Yassour, M. and Thompson, D. A. and Haas, B. J. and Habib, N. and Wapinski, I. and Roy, S. and Lin, M. F. and Heiman, D. I. and Young, S. K. and Furuya, K. and Guo, Y. B. and Pidoux, A. and Chen, H. M. and Robbertse, B. and Goldberg, J. M. and Aoki, K. and Bayne, E. H. and Berlin, A. M. and Desjardins, C. A. and Dobbs, E. and Dukaj, L. and Fan, L. and FitzGerald, M. G. and French, C. and Gujja, S. and Hansen, K. and Keifenheim, D. and Levin, J. Z. and Mosher, R. A. and Muller, C. A. and Pfiffner, J. and Priest, M. and Russ, C. and Smialowska, A. and Swoboda, P. and Sykes, S. M. and Vaughn, M. and Vengrova, S. and Yoder, R. and Zeng, Q. D. and Allshire, R. and Baulcombe, D. and Birren, B. W. and Brown, W. and Ekwall, K. and Kellis, M. and Leatherwood, J. and Levin, H. and Margalit, H. and Martienssen, R. and Nieduszynski, C. A. and Spatafora, J. W. and Friedman, N. and Dalgaard, J. Z. and Baumann, P. and Niki, H. and Regev, A. and Nusbaum, C.},
	title	    = {Comparative Functional Genomics of the Fission Yeasts RID E-6356-2011 RID F-4800-2010 RID B-2170-2010},
	journal     = {Science},
	journal-iso	= {Science},
	keywords     = {SCHIZOSACCHAROMYCES-POMBE; REPLICATION ORIGINS; GENES; HETEROCHROMATIN; CENTROMERE; RNAI; TRANSCRIPTOME; EXPRESSION; INTEGRITY; PROTEINS},
	year	    = {2011},
	volume	    = {332},
	pages	    = {930-936},
	abstract    = {The fission yeast clade-comprising Schizosaccharomyces pombe, S. octosporus, S. cryophilus, and S. japonicus-occupies the basal branch of Ascomycete fungi and is an important model of eukaryote biology. A comparative annotation of these genomes identified a near extinction of transposons and the associated innovation of transposon-free centromeres. Expression analysis established that meiotic genes are subject to antisense transcription during vegetative growth, which suggests a mechanism for their tight regulation. In addition, trans-acting regulators control new genes within the context of expanded functional modules for meiosis and stress response. Differences in gene content and regulation also explain why, unlike the budding yeast of Saccharomycotina, fission yeasts cannot use ethanol as a primary carbon source. These analyses elucidate the genome structure and gene regulation of fission yeast and provide tools for investigation across the Schizosaccharomyces clade.
	  },
	file	={Rhind.et.al.11.pdf:/home/mikeg/References/Rhind.et.al.11.pdf:PDF},
	timestamp	={2011.9.20}
}

@article{McFarlaneAndHumphrey2010,
	author	    = {McFarlane, R. J. and Humphrey, T. C.},
	title	    = {A role for recombination in centromere function},
	journal     = {Trends In Genetics},
	journal-iso	= {Trends Genet.},
	keywords     = {TRANSFER-RNA GENES; FISSION YEAST CENTROMERE; CENP-A; SCHIZOSACCHAROMYCES-POMBE; DNA-REPLICATION; INTERGENIC CONVERSION; HUMAN NEOCENTROMERE; CHROMATIN STRUCTURE; CELL-CYCLE; HETEROCHROMATIN},
	year	    = {2010},
	volume	    = {26},
	pages	    = {209-213},
	abstract    = {Centromeres are essential for chromosome segregation during both mitosis and meiosis. There are no obvious or conserved DNA sequence motif determinants for centromere function, but the complex centromeres found in the majority of eukaryotes studied to date consist of repetitive DNA sequences. A striking feature of these repeats is that they maintain a high level of inter-repeat sequence identity within the centromere. This observation is suggestive of a recombination mechanism that operates at centromeres. Here we postulate that inter-repeat homologous recombination plays an intrinsic role in centromere function by forming covalently closed DNA loops. Moreover, the model provides an explanation of why both inverted and direct repeats are maintained and how they contribute to centromere function.
	  },
	file	={McFarlane.and.Humphrey.10.pdf:/home/mikeg/References/McFarlane.and.Humphrey.10.pdf:PDF},
	timestamp	={2011.9.20}
}

@article{BuscainoEtAl2010,
	author	    = {Buscaino, A. and Allshire, R. and Pidoux, A.},
	title	    = {Building centromeres: home sweet home or a nomadic existence?},
	journal     = {Current Opinion In Genetics \& Development},
	journal-iso	= {Curr. Opin. Genet. Dev.},
	keywords     = {HISTONE H3 VARIANT; FISSION YEAST SCM3; CENP-A NUCLEOSOMES; CHROMOSOME SEGREGATION; BUDDING YEAST; FUNCTIONAL CENTROMERES; CHROMATIN; KINETOCHORE; DNA; SPECIFICATION},
	year	    = {2010},
	volume	    = {20},
	pages	    = {118-126},
	abstract    = {Centromere assembly and propagation is governed by genetic and epigenetic mechanisms. A centromere-specific histone H3 variant, CENP-A is strongly favored as the epigenetic mark that specifies centromere identity. Despite the critical importance of centromere function, centromeric sequences are not conserved. This has prompted exploration of other genomic and chromatin features to gain an understanding of where CENP-A is deposited. In this review we highlight recent papers that advance our understanding of how the cell builds a centromere. We focus on what influences the choice of site for CENP-A deposition and therefore the site of centromere formation. We then briefly discuss how centromeres are propagated once the site of centromere assembly is chosen.
	  },
	file	={Buscaino.et.al.10.pdf:/home/mikeg/References/Buscaino.et.al.10.pdf:PDF},
	timestamp	={2011.9.20}
}

@article{JacoEtAl2008,
	author	    = {Jaco, I. and Canela, A. and Vera, E. and Blasco, M. A.},
	title	    = {Centromere mitotic recombination in mammalian cells},
	journal     = {Journal Of Cell Biology},
	journal-iso	= {J. Cell Biol.},
	keywords     = {MAJOR SATELLITE REPEATS; B NULL MICE; CENP-B; MINOR SATELLITE; DNA METHYLATION; PERICENTRIC HETEROCHROMATIN; TELOMERE RECOMBINATION; HUMAN-CHROMOSOMES; MOUSE; SEQUENCE},
	year	    = {2008},
	volume	    = {181},
	pages	    = {885-892},
	abstract    = {Centromeres are special structures of eukaryotic chromosomes that hold sister chromatid together and ensure proper chromosome segregation during cell division. Centromeres consist of repeated sequences, which have hindered the study of centromere mitotic recombination and its consequences for centromeric function. We use a chromosome orientation fluorescence in situ hybridization technique to visualize and quantify recombination events at mouse centromeres. We show that centromere mitotic recombination occurs in normal cells to a higher frequency than telomere recombination and to a much higher frequency than chromosome-arm recombination. Furthermore, we show that centromere mitotic recombination is increased in cells lacking the Dnmt3a and Dnmt3b DNA methyltransferases, suggesting that the epigenetic state of centromeric heterochromatin controls recombination events at these regions. Increased centromere recombination in Dnmt3a, 3b-deficient cells is accompanied by changes in the length of centromere repeats, suggesting that prevention of illicit centromere recombination is important to maintain centromere integrity in the mouse.},
	file	={Jaco.et.al.08.pdf:/home/mikeg/References/Jaco.et.al.08.pdf:PDF},
	timestamp	={2011.9.20}
}

@article{FolcoEtAl2008,
	author	    = {Folco, H. D. and Pidoux, A. L. and Urano, T. and Allshire, R. C.},
	title	    = {Heterochromatin and {RNA}i are required to establish CENP-A chromatin at centromeres},
	journal     = {Science},
	journal-iso	= {Science},
	keywords     = {FISSION YEAST; CHROMOSOME SEGREGATION; LYSINE-9 METHYLATION; PROTEIN; RITS; PROPAGATION; DISTINCT; DOMAINS; COMPLEX; ACTS},
	year	    = {2008},
	volume	    = {319},
	pages	    = {94-97},
	abstract    = {Heterochromatin is defined by distinct posttranslational modifications on histones, such as methylation of histone H3 at lysine 9 ( H3K9), which allows heterochromatin protein 1 ( HP1)- related chromodomain proteins to bind. Heterochromatin is frequently found near CENP-A chromatin, which is the key determinant of kinetochore assembly. We have discovered that the RNA interference ( RNAi)- directed heterochromatin flanking the central kinetochore domain at fission yeast centromeres is required to promote CENPA(Cnp1) and kinetochore assembly over the central domain. The H3K9 methyltransferase Clr4 ( Suv39); the ribonuclease Dicer, which cleaves heterochromatic double- stranded RNA to small interfering RNA ( siRNA); Chp1, a component of the RNAi effector complex ( RNA- induced initiation of transcriptional gene silencing; RITS); and Swi6 ( HP1) are required to establish CENP-A(Cnp1) chromatin on naive templates. Once assembled, CENP-A(Cnp1) chromatin is propagated by epigenetic means in the absence of heterochromatin. Thus, another, potentially conserved, role for centromeric RNAi- directed heterochromatin has been identified.
	  },
	file	={Folco.et.al.08.pdf:/home/mikeg/References/Folco.et.al.08.pdf:PDF},
	timestamp	={2011.9.20}
}

@article{LejeuneEtAl2007,
	author	    = {Lejeune, E. and Bortfeld, M. and White, S. A. and Pidoux, A. L. and Ekwall, K. and Allshire, R. C. and Ladurner, A. G.},
	title	    = {The chromatin-remodeling factor FACT contributes to centromeric heterochromatin independently of {RNA}i},
	journal     = {Current Biology},
	journal-iso	= {Curr. Biol.},
	keywords     = {FISSION YEAST; CHROMOSOME SEGREGATION; COMPLEX; MUTATIONS; METHYLATION; DISRUPT; GENES; RSC},
	year	    = {2007},
	volume	    = {17},
	pages	    = {1219-1224},
	abstract    = {Centromeres exert vital cellular functions in mitosis and meiosis. A specialized histone and other chromatin-bound factors nucleate a dynamic protein assembly that is required for the proper segregation of sister chromatids. In several organisms, including the fission yeast, Schizosaccharomyces pombe, the RNAi pathway contributes to the formation of silent chromatin in pericentromeric regions. Little is known about how chromatin-remodeling factors contribute to heterochromatic integrity and centromere function. Here we show that the histone chaperone and remodeling complex FACT is required for centromeric-heterochromatin integrity and accurate chromosome segregation. We show that Spt16 and Pob3 are two subunits of the S. pombe FACT complex. Surprisingly, yeast strains deleted for pob3+ are viable and alleviate gene silencing at centromeric repeats and at the silent mating-type locus. Importantly, like heterochromatin and RNAi pathway mutants, Pob3 null strains exhibit lagging chromosomes on anaphase spindles. Whereas the processing of centromeric RNA transcripts into siRNAs is maintained in Pob3 mutants, Swi6-association with the centromere is reduced. Our studies provide the first experimental evidence for a role of the RNA polymerase II cofactor FACT in heterochromatin integrity and in centromere function.},
	file	={Lejeune.et.al.07.pdf:/home/mikeg/References/Lejeune.et.al.07.pdf:PDF},
	timestamp	={2011.9.20}
}

@article{DunleavyEtAl2005,
	author	    = {Dunleavy, E. and Pidoux, A. and Allshire, R.},
	title	    = {Centromeric chromatin makes its mark},
	journal     = {Trends In Biochemical Sciences},
	journal-iso	= {Trends Biochem.Sci.},
	keywords     = {HISTONE H3; CENP-A; ACTIVE GENES; LYSINE 9; METHYLATION; IDENTITY; DISTINCT; DOMAIN; CODE},
	year	    = {2005},
	volume	    = {30},
	pages	    = {172-175},
	abstract    = {Post-translational modifications of core histones, particularly in the N terminus, seem to define different chromatin states in the genome. For instance, actively transcribed regions of euchromatin have a completely different set of ' marks ' compared with silent heterochromatin. Recent analyses surprisingly demonstrate that, in addition to containing the histone H3 variant CENP-A, the centromeric chromatin that underlies the kinetochore bears a distinct combination of histone H3 modifications.},
